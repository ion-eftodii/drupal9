<?php

/**
 * @file
 * Functions to support theming in the Stable subtheme.
 */

/**
 * Implements hook_preprocess_HOOK() for node.html.twig.
 */
function stable_subtheme_preprocess_node(&$variables) {
  $variables['#attached']['library'][] = 'stable_subtheme/card-styles';
  $variables['#attached']['library'][] = 'stable_subtheme/card-decoupled';
}
